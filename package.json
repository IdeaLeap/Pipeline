{
  "name": "@idealeap/pipeline",
  "version": "1.1.3",
  "description": "A lightweight, low-code Pipeline built with GPT-4.",
  "main": "package/index.js",
  "author": "GPT-4 && Marlene && IdeaLeap",
  "repository": "github:idealeap/Pipeline",
  "license": "MIT",
  "type": "module",
  "private": false,
  "keywords": [
    "Typescript",
    "pipeline",
    "low code",
    "lightweight"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "ts-node --esm ./package/index.ts",
    "test": "jest --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint --ext .ts,.js ./",
    "publish": "(cd dist && npm publish --access public)",
    "fix": "eslint --fix --ext ts,js .",
    "tsn": "ts-node -r tsconfig-paths/register",
    "format": "prettier --write --cache  --cache-strategy metadata . !dist",
    "build": "bash ./build"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@idealeap/pipeline": "link:./package",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@microsoft/api-documenter": "^7.23.2",
    "@microsoft/api-extractor": "^7.37.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsc-multi": "^1.1.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vitepress": "^1.0.0-beta.5"
  }
}
